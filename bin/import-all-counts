#!/usr/bin/env bash

echo importing student actions...
cat data/student-actions-per-minute.json | ./bin/import-counts actions student_counts
echo importing student hits...
cat data/student-hits-per-minute.json | ./bin/import-counts hits student_counts
echo importing student uniques...
cat data/student-uniques-per-minute.json | ./bin/import-counts uniques student_counts

echo importing instructor actions...
cat data/instructor-actions-per-minute.json | ./bin/import-counts actions instructor_counts
echo importing instructor hits...
cat data/instructor-hits-per-minute.json | ./bin/import-counts hits instructor_counts
echo importing instructor uniques...
cat data/instructor-uniques-per-minute.json | ./bin/import-counts uniques instructor_counts

echo importing actions...
cat data/actions-per-minute.json | ./bin/import-counts actions
echo importing hits...
cat data/hits-per-minute.json | ./bin/import-counts hits
echo importing uniques...
cat data/uniques-per-minute.json | ./bin/import-counts uniques
